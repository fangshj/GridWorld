//  This class is used to store the linked list's entry of Grid

public class OccupantInCol
{
    private Object occupant;
    private int col;

	private OccupantInCol(Object obj, int colNew) {
		occupant = obj;
		col = colNew;
	}
	public Object getObject() {
		return occupant;	
	}
	public int getCol() {
		return col;	
	}

	public void setObject(Object obj) {
		occupant = obj;	
	}
	public void setCol(int colNew) {
		col = colNew;	
	}
}
